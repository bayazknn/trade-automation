{
  "metadata": {
    "version": "1.1",
    "description": "Technical indicators for spot trading strategy generation",
    "library": "TA-Lib",
    "trading_mode": "spot",
    "created": "2026-01-05"
  },
  "operators": {
    "lt": "<",
    "gt": ">",
    "lte": "<=",
    "gte": ">=",
    "crosses_above": "qtpylib.crossed_above()",
    "crosses_below": "qtpylib.crossed_below()"
  },
  "indicators": {
    "RSI": {
      "name": "Relative Strength Index",
      "category": "momentum",
      "talib_function": "ta.RSI",
      "params": {
        "timeperiod": {"default": 14, "range": [2, 30], "type": "int"}
      },
      "outputs": ["rsi"],
      "entry": {
        "signal_type": "threshold",
        "condition": {"left": "rsi", "operator": "lt", "right": "constant"},
        "constant": {"default": 30, "range": [20, 40]}
      },
      "exit": {
        "signal_type": "threshold",
        "condition": {"left": "rsi", "operator": "gt", "right": "constant"},
        "constant": {"default": 70, "range": [60, 80]}
      }
    },
    "MACD": {
      "name": "Moving Average Convergence Divergence",
      "category": "momentum",
      "talib_function": "ta.MACD",
      "params": {
        "fastperiod": {"default": 12, "range": [5, 20], "type": "int"},
        "slowperiod": {"default": 26, "range": [15, 40], "type": "int"},
        "signalperiod": {"default": 9, "range": [5, 15], "type": "int"}
      },
      "outputs": ["macd", "macdsignal", "macdhist"],
      "entry": {
        "signal_type": "crossover",
        "condition": {"left": "macd", "operator": "crosses_above", "right": "macdsignal"}
      },
      "exit": {
        "signal_type": "crossover",
        "condition": {"left": "macd", "operator": "crosses_below", "right": "macdsignal"}
      }
    },
    "BBANDS": {
      "name": "Bollinger Bands",
      "category": "overlap",
      "talib_function": "ta.BBANDS",
      "params": {
        "timeperiod": {"default": 20, "range": [10, 50], "type": "int"},
        "nbdevup": {"default": 2.0, "range": [1.0, 3.0], "type": "float"},
        "nbdevdn": {"default": 2.0, "range": [1.0, 3.0], "type": "float"}
      },
      "outputs": ["upperband", "middleband", "lowerband"],
      "entry": {
        "signal_type": "threshold",
        "condition": {"left": "close", "operator": "lt", "right": "lowerband * factor"},
        "factor": {"default": 1.02, "range": [1.0, 1.05]}
      },
      "exit": {
        "signal_type": "threshold",
        "condition": {"left": "close", "operator": "gt", "right": "upperband * factor"},
        "factor": {"default": 0.98, "range": [0.95, 1.0]}
      }
    },
